  "environments": {
    "EXPO_PUBLIC_SUPABASE_URL": "required",
    "EXPO_PUBLIC_SUPABASE_ANON_KEY": "required",
    "EXPO_PUBLIC_RC_API_KEY_IOS": "required",
    "EXPO_PUBLIC_RC_API_KEY_ANDROID": "required"
