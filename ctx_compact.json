    "apps/mobile/src/hooks/useStorefront.ts": {
      "purpose": "RevenueCat integration hook",
      "risk": "platform API key handling, entitlement parsing",
      "tests": "offerings load, purchase flows"
    },
    "apps/mobile/src/lib/data.ts": {
      "purpose": "Data layer with DI queue integration",
      "risk": "queue enqueue function binding",
      "tests": "mutation queueing"
    }
