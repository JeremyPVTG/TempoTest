{
  "overview": "Expo SDK 53 mobile app with AsyncStorage offline queue, expo-router, RevenueCat integration",
  "files": {
    "apps/mobile/package.json": {
      "purpose": "Expo SDK 53 dependencies and scripts",
      "risk": "dependency version conflicts",
      "tests": "workspace integration"
    },
    "apps/mobile/app.config.ts": {
      "purpose": "Expo configuration with typed routes",
      "risk": "environment variable access",
      "tests": "config validation"
    },
    "apps/mobile/metro.config.js": {
      "purpose": "Monorepo symlink resolution",
      "risk": "workspace package resolution",
      "tests": "import resolution"
    },
    "apps/mobile/app/_layout.tsx": {
      "purpose": "Root layout with providers",
      "risk": "provider initialization order",
      "tests": "provider mounting"
    },
    "packages/data/src/offlineQueue/storage/asyncStorageDriver.ts": {
      "purpose": "Shared AsyncStorage driver",
      "risk": "JSON parse errors, storage failures",
